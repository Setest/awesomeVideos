    "lexicon": "awesomeVideos:properties",
    "overridden": false,
    "desc_trans": "Разделитель для условий AND в параметре \"&tvFilters\". По умолчанию: \",\".",
    "area": "TV",
    "area_trans": "TV",
    "menu": null
